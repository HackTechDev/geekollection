{% extends 'base.html.twig' %}
  
{% block title %}Project index{% endblock %}
  
{% block body %}
    <div class="container">
        <h2 class="text-center mt-5 mb-3">Search Item</h2>
        <div class="card">
            <div class="card-header">

            </div>
            <div class="card-body">
                {{ form_start(form) }}
                <div class="form-group">
                    {{ form_row(form.inputsearch, {'attr': {'class': 'form-control'}}) }}
                </div>
                <button class="btn {{ button_color|default('btn-outline-primary') }}  mt-3">{{ button_label|default('Search') }}</button>
                {{ form_end(form) }}
            </div>
        </div>
    </div>
  
{% endblock %}