{% extends 'base.html.twig' %}
  
{% block title %}Oeuvrelink{% endblock %}
  
{% block body %}
    <div class="container">
        <h2 class="text-center mt-5 mb-3">Show Oeuvrelink</h2>
        <div class="card">
            <div class="card-header">
                <a class="btn btn-outline-info float-right" href="{{ path('app_oeuvrelink_index') }}"> 
                    View All Oeuvrelinks
                </a>
            </div>
            <div class="card-body">
                <b class="text-muted">Link:</b>
                <p>{{ oeuvrelink.link }}</p>
                <b class="text-muted">Date Created:</b>
                <p>{{ oeuvrelink.createdAt ? oeuvrelink.createdAt|date('Y-m-d H:i:s') : '' }}</p>
                <b class="text-muted">Date Updated:</b>
                <p>{{ oeuvrelink.updatedAt ? oeuvrelink.updatedAt|date('Y-m-d H:i:s') : '' }}</p>
            </div>
        </div>
    </div>
{% endblock %}
